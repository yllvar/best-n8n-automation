# n8n Automation Portfolio - Environment Variables
# Copy this file to .env and fill in your actual values

# =============================================================================
# n8n Configuration
# =============================================================================
# n8n execution mode (default: regular)
N8N_EXECUTION_MODE=regular

# n8n host (default: localhost)
N8N_HOST=localhost

# n8n port (default: 5678)
N8N_PORT=5678

# n8n protocol (http/https)
N8N_PROTOCOL=http

# =============================================================================
# AI & LLM Integration
# =============================================================================

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Google AI / Gemini API Key
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Anthropic Claude API Key
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# DeepSeek API Key
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# NeurochainAI API Key
NEUROCHAINAI_API_KEY=your_neurochainai_api_key_here

# ElevenLabs API Key
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# =============================================================================
# Database & Storage
# =============================================================================

# PostgreSQL Connection
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=your_database_name
POSTGRES_USER=your_username
POSTGRES_PASSWORD=your_password

# MongoDB Connection
MONGODB_URI=mongodb://localhost:27017/your_database_name

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Pinecone API Key
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment

# =============================================================================
# Messaging Platforms
# =============================================================================

# Telegram Bot Token
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# WhatsApp Business API
WHATSAPP_PHONE_NUMBER_ID=your_whatsapp_phone_number_id
WHATSAPP_ACCESS_TOKEN=your_whatsapp_access_token
WHATSAPP_WEBHOOK_VERIFY_TOKEN=your_whatsapp_webhook_verify_token

# Discord Bot Token
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_CLIENT_ID=your_discord_client_id_here
DISCORD_GUILD_ID=your_discord_guild_id_here

# Slack Configuration
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_SIGNING_SECRET=your-slack-signing-secret

# =============================================================================
# Google Workspace
# =============================================================================

# Google Cloud Project
GOOGLE_PROJECT_ID=your_google_cloud_project_id

# Google Service Account (JSON key file path)
GOOGLE_SERVICE_ACCOUNT_KEY_PATH=/path/to/your/service-account-key.json

# Google Sheets API
GOOGLE_SHEETS_API_KEY=your_google_sheets_api_key_here

# Google Drive API
GOOGLE_DRIVE_API_KEY=your_google_drive_api_key_here

# =============================================================================
# Social Media Platforms
# =============================================================================

# Twitter/X API
TWITTER_API_KEY=your_twitter_api_key_here
TWITTER_API_SECRET=your_twitter_api_secret_here
TWITTER_ACCESS_TOKEN=your_twitter_access_token_here
TWITTER_ACCESS_TOKEN_SECRET=your_twitter_access_token_secret_here
TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here

# Instagram Basic Display API
INSTAGRAM_APP_ID=your_instagram_app_id_here
INSTAGRAM_APP_SECRET=your_instagram_app_secret_here
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token_here

# Reddit API
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here
REDDIT_USER_AGENT=your_reddit_user_agent_here

# =============================================================================
# Content & Media
# =============================================================================

# BannerBear API
BANNERBEAR_API_KEY=your_bannerbear_api_key_here

# WordPress
WORDPRESS_URL=your_wordpress_site_url
WORDPRESS_USERNAME=your_wordpress_username
WORDPRESS_PASSWORD=your_wordpress_password

# =============================================================================
# Email & Communication
# =============================================================================

# SendGrid API
SENDGRID_API_KEY=your_sendgrid_api_key_here

# Resend API
RESEND_API_KEY=your_resend_api_key_here

# =============================================================================
# Productivity & Business Tools
# =============================================================================

# Airtable API
AIRTABLE_API_KEY=your_airtable_api_key_here
AIRTABLE_BASE_ID=your_airtable_base_id_here

# Notion API
NOTION_API_KEY=your_notion_integration_token_here

# Linear API
LINEAR_API_KEY=your_linear_api_key_here

# HubSpot API
HUBSPOT_API_KEY=your_hubspot_api_key_here

# Pipedrive API
PIPEDRIVE_API_TOKEN=your_pipedrive_api_token_here

# =============================================================================
# Monitoring & Analytics
# =============================================================================

# Jina AI API
JINA_AI_API_KEY=your_jina_ai_api_key_here

# Perplexity API
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# =============================================================================
# Development & Testing
# =============================================================================

# Environment (development/staging/production)
NODE_ENV=development

# Enable debug logging
DEBUG=n8n:*

# =============================================================================
# Security
# =============================================================================

# JWT Secret for n8n (generate a strong random string)
N8N_JWT_SECRET=your_jwt_secret_here

# Encryption key for workflow credentials
N8N_ENCRYPTION_KEY=your_encryption_key_here

# =============================================================================
# Optional: External Services
# =============================================================================

# Redis (for caching)
REDIS_URL=redis://localhost:6379

# Webhook URL base
WEBHOOK_URL_BASE=https://your-domain.com/webhook

# Custom user folder
N8N_USER_FOLDER=/home/node/.n8n

# =============================================================================
# Notes
# =============================================================================
# 1. Never commit your actual .env file to version control
# 2. Use strong, unique passwords and API keys
# 3. Rotate API keys regularly for security
# 4. Some workflows may require additional environment variables
# 5. Refer to individual workflow documentation for specific requirements
